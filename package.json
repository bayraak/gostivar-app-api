   "main": "build/index.js",
   "scripts": {
      "tsc": "tsc",
      "start": "set debug=* && ts-node-dev --respawn --transpileOnly ./src/index.ts",
      "postinstall": "npm run tsc",
      "prod": "tsc && node ./build/index.js",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
      "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
      "node:build": "node ./build/index.js",
      "admin:dev": "set debug=error && ts-node-dev --respawn --transpileOnly ./src/admin/index.ts",
      "admin:prod": "set debug=error && tsc && node ./build/admin/index.js"
   }
}
